{
  "name": "learn-vue3",
  "version": "0.0.1",
  "description": "Learn vue3 & express",
  "private": true,
  "author": "kenux",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
